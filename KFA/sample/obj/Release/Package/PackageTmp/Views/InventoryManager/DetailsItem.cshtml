@model sample.Models.ProductsetupModel
@{
    ViewBag.Title = "DetailsItem";
}
<div>
    @if (Session["exist"] != null)
    {
        <div class="alert alert-success">
            @Session["exist"].ToString();
            <a href="#" class="close">&times;</a>
        </div>
    }
    @if (Session["fail"] != null)
    {
        <div class="alert alert-danger">
            @Session["fail"].ToString();
            <a href="#" class="close">&times;</a>
        </div>
    }
</div>
<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading"><h4>Item Details</h4></div>
        <div class="panel-body">
            <div class="col-lg-offset-1">
                <div>
                    @Html.LabelFor(x => x.productName)
                    <br />
                    @Html.DisplayFor(model => model.productName, new { @class = "form-control  input-lg", @readonly = "readonly" })
                    <br />
                    @Html.LabelFor(x => x.categoryName)
                    <br />
                    @Html.DisplayFor(model => model.categoryName, new { @class = "form-control  input-lg", @readonly = "readonly" })
                    <br />
                    @Html.LabelFor(x => x.subcategory)
                    <br />
                    @Html.DisplayFor(model => model.subcategory, new { @class = "form-control  input-lg" })
                    <br />
                    @Html.LabelFor(x => x.price)
                    <br />
                    @Html.DisplayFor(model => model.price, new { @class = "form-control  input-lg" })
                    <br />
                    @Html.LabelFor(x => x.quantity)
                    <br />
                    @Html.DisplayFor(model => model.quantity, new { @class = "form-control  input-lg" })
                    <br />
                </div>
            </div>
        </div>
    </div>
</div>